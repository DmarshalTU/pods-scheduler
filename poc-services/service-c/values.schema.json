{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "dependencies": {
        "type": "array",
        "description": "OCS service dependencies",
        "items": {
          "type": "string",
          "enum": ["service-a", "service-b"]  
        }
      },
      "initContainers": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean",
            "default": true
          }
        }
      }
    }
  }